{
  "name": "online-store",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "npx husky install",
    "generate": "npx prisma generate",
    "build": "npm run generate && nest build",
    "migrate": "npx prisma migrate deploy",
    "seed": "npx prisma db seed",
    "start": "node dist/src/main ",
    "test": "npm run test:int && npm run test:e2e",
    "lint": "npx eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "build:dev": "nest build --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "migrate:dev": "npx prisma migrate dev",
    "test:e2e": "npx jest --passWithNoTests --config ./test/e2e/jest-e2e.json",
    "test:int": "npx jest --passWithNoTests --config ./test/integration/jest-int.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.367.0",
    "@aws-sdk/lib-storage": "^3.367.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.0.5",
    "@nestjs/schedule": "^3.0.1",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/swagger": "^7.1.1",
    "@nestjs/terminus": "^10.0.1",
    "@nestjs/testing": "^10.0.0",
    "@nestjs/throttler": "^4.2.1",
    "@prisma/client": "^5.18.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "google-auth-library": "^8.9.0",
    "handlebars": "^4.7.8",
    "helmet": "^7.0.0",
    "jest": "^29.5.0",
    "morgan": "^1.10.0",
    "mysqldump": "^3.2.0",
    "nodemailer": "^6.9.3",
    "prisma-class-generator": "^0.2.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "supertest": "^6.3.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.2",
    "@types/nodemailer": "^6.4.8",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-autofix": "^1.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "prisma": "^5.18.0",
    "source-map-support": "^0.5.21",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "prisma": {
    "seed": "node dist/prisma/seeds/main.seed.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "rootDir": ".",
    "testTimeout": 10000,
    "testEnvironment": "node",
    "testRegex": ".(e2e|int|unit)-spec.js$"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
}
